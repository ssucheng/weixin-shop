.carousel swiper {
  width: 750rpx;
  height: 340rpx;
}
.carousel swiper image {
  width: 100%;
  height: 100%;
}
.nav {
  display: flex;
}
.nav .navItem {
  flex: 1;
  padding: 20rpx;
}
.nav .navItem image {
  width: 100%;
}
.floor .floorItem .floorItem_tit {
  padding: 10rpx 0;
}
.floor .floorItem .floorItem_tit image {
  width: 100%;
}
.floor .floorItem .floorItem_content {
  overflow: hidden;
  margin-left: 6rpx;
}
.floor .floorItem .floorItem_content navigator {
  height: 55.45422414vw;
  width: 33.33%;
  float: left;
}
.floor .floorItem .floorItem_content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid white;
}
.floor .floorItem .floorItem_content navigator:nth-child(2),
.floor .floorItem .floorItem_content navigator:nth-child(3) {
  border-bottom: 10rpx solid white;
}
.floor .floorItem .floorItem_content navigator image {
  width: 100%;
  height: 100%;
}
